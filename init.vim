syntax on
highlight Comment cterm=italic

let mapleader = "'"
let g:UltiSnipsExpandTrigger="<tab>"
let g:UltiSnipsListSnippets="<c-l>"
let g:airline_powerline_fonts = 1
let g:airline#extensions#tabline#enab
let g:airline#extensions#tabline#show

set shell=/bin/zsh
set number
set wildmode=longest,list
set hlsearch
set ignorecase
set incsearch
set noswapfile
set noshowmode
set tabstop=4
set expandtab
set wrap
